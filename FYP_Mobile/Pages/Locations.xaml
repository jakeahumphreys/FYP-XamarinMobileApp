<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FYP_Mobile.Pages.Locations"
             x:Name="LocationPage">
<ListView x:Name="LocationsView" ItemTapped="LocationsListItemTapped" ItemsSource="{Binding LocationsList}">
    <ListView.ItemTemplate>
        <DataTemplate>
            <TextCell Text="{Binding Label}"></TextCell>
        </DataTemplate>
    </ListView.ItemTemplate>
</ListView>
</ContentPage>